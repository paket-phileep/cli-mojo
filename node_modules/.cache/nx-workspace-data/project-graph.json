{
  "nodes": {
    "cli-mojo": {
      "name": "cli-mojo",
      "type": "lib",
      "data": {
        "root": "pkg",
        "sourceRoot": "pkg",
        "name": "cli-mojo",
        "tags": [
          "npm:public"
        ],
        "metadata": {
          "targetGroups": {
            "NPM Scripts": [
              "test",
              "build",
              "prepublish",
              "coveralls"
            ]
          },
          "description": "a customizable progress bar for ink"
        },
        "targets": {
          "test": {
            "executor": "nx:run-script",
            "options": {
              "script": "test"
            },
            "metadata": {
              "scriptContent": "xo --fix && jest --coverage",
              "runCommand": "yarn test"
            },
            "configurations": {},
            "parallelism": true
          },
          "build": {
            "executor": "nx:run-script",
            "options": {
              "script": "build"
            },
            "metadata": {
              "scriptContent": "babel src --out-dir lib --ignore src/__tests__",
              "runCommand": "yarn build"
            },
            "configurations": {},
            "parallelism": true
          },
          "prepublish": {
            "executor": "nx:run-script",
            "options": {
              "script": "prepublish"
            },
            "metadata": {
              "scriptContent": "npm run test && npm run build",
              "runCommand": "yarn prepublish"
            },
            "configurations": {},
            "parallelism": true
          },
          "coveralls": {
            "executor": "nx:run-script",
            "options": {
              "script": "coveralls"
            },
            "metadata": {
              "scriptContent": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
              "runCommand": "yarn coveralls"
            },
            "configurations": {},
            "parallelism": true
          },
          "nx-release-publish": {
            "dependsOn": [
              "^nx-release-publish"
            ],
            "executor": "@nx/js:release-publish",
            "options": {},
            "configurations": {},
            "parallelism": true
          }
        },
        "implicitDependencies": []
      }
    }
  },
  "externalNodes": {},
  "dependencies": {
    "cli-mojo": []
  },
  "version": "6.0"
}